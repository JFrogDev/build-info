{
  "files": [
    {
      "pattern": "files/a/b/*",
      "target": "${LOCAL_REPO}/",
      "recursive": "true",
      "excludePatterns": ["files/a/"]
    },
    {
      "pattern": "files/a/*",
      "target": "${LOCAL_REPO}/",
      "recursive": "true",
      "exclusions": ["files/*/a1.in", "files/*/*2.in", "files/*/*3*"]
    }
  ]
}